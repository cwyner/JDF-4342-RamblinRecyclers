{
  "expo": {
    "name": "artifact-impl-demo",
    "slug": "artifact-impl-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/upcycling.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.artifactimpldemo",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/upcycling.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.artifactimpldemo",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-secure-store",
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
